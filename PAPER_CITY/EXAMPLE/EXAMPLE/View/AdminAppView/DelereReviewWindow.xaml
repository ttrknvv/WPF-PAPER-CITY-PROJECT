<Window x:Class="EXAMPLE.View.AdminAppView.DelereReviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EXAMPLE.View.AdminAppView"
        mc:Ignorable="d"
        Title="DeleteReviewWindow" 
        Height="100" 
        Width="300"
        Background="#FFB8CEE7"
        ResizeMode="NoResize" 
        WindowStartupLocation="CenterScreen">
    <Grid>
        <WrapPanel HorizontalAlignment="Center"
                   VerticalAlignment="Center">
            <Button Content="{DynamicResource DeleteText}"
                    Style="{DynamicResource ButtonBuy}"
                    Click="Button_Click"/>
            <Button Content="{DynamicResource Cansel}"
                    Style="{DynamicResource ButtonBuy}"
                    Margin="30, 0, 0, 0"
                    Click="Button_Click_1"/>
        </WrapPanel>
    </Grid>
</Window>
